{
  "metadata": {
    "spec-version": "1.4"
  },
  "configuration-groups": [
    {
      "label": "Sink Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Reference Name",
          "name": "referenceName"
        },
        {
          "widget-type": "textbox",
          "label": "Hosts",
          "name": "hosts",
          "widget-attributes": {
            "default": "localhost"
          }
        },
        {
          "widget-type": "number",
          "label": "Port",
          "name": "port",
          "widget-attributes": {
            "default": "8182"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Remote Connection Class",
          "name": "remoteConnectionClass",
          "widget-attributes": {
            "default": "org.apache.tinkerpop.gremlin.driver.remote.DriverRemoteConnection"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Serializer Classname",
          "name": "serializerClassName",
          "widget-attributes": {
            "default": "org.apache.tinkerpop.gremlin.driver.ser.GryoMessageSerializerV3d0"
          }
        },
        {
          "widget-type": "textbox",
          "label": "IO Registry",
          "name": "ioRegistries",
          "widget-attributes": {
            "default": "org.janusgraph.graphdb.tinkerpop.JanusGraphIoRegistry"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Graph Source Name",
          "name": "graphSourceName",
          "widget-attributes": {
            "default": "g"
          }
        },
        {
          "name": "recordToVertexMapper",
          "label": "Record To Vertex Mapper",
          "widget-type": "json-editor",
          "widget-attributes": {
            "default": "{\n  \"NODE_LIST\": [\n    {\n      \"label\": \"Field1\",\n      \"id\": \"Field2\",\n      \"isHardCodedLabel\": true,\n      \"properties\": [\n        \"Field3\",\n        \"Field4\"\n      ]\n    }\n  ],\n  \"EDGE_LIST\": [\n    {\n      \"label\": \"Field1\",\n      \"id\": \"Field2\",\n      \"isHardCodedLabel\": true,\n      \"fromLabel\": \"fromLabel\",\n      \"toLabel\": \"toLabel\",\n      \"properties\": [\n        \"Field3\",\n        \"Field4\"\n      ]\n    }\n  ]\n}"
          }
        },
        {
          "widget-type": "number",
          "label": "Batch Size",
          "name": "batchSizeConfig",
          "widget-attributes": {
            "default": "1000"
          }
        },
        {
          "widget-type": "connection-browser",
          "widget-category": "plugin",
          "widget-attributes": {
            "connectionType": "Janus",
            "label": "Browse Connections"
          }
        }
      ]
    }
  ],
  "outputs": []
}
