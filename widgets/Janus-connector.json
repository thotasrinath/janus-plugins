{
  "metadata": {
    "spec-version": "1.0"
  },
  "display-name": "JanusGraph",
  "configuration-groups": [
    {
      "label": "Sink Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Reference Name",
          "name": "referenceName"
        },
        {
          "widget-type": "textbox",
          "label": "Hosts",
          "name": "hosts",
          "widget-attributes": {
            "default": "localhost"
          }
        },
        {
          "widget-type": "number",
          "label": "Port",
          "name": "port",
          "widget-attributes": {
            "default": "8182"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Remote Connection Class",
          "name": "remoteConnectionClass",
          "widget-attributes": {
            "default": "org.apache.tinkerpop.gremlin.driver.remote.DriverRemoteConnection"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Serializer Classname",
          "name": "serializerClassName",
          "widget-attributes": {
            "default": "org.apache.tinkerpop.gremlin.driver.ser.GryoMessageSerializerV3d0"
          }
        },
        {
          "widget-type": "textbox",
          "label": "IO Registry",
          "name": "ioRegistries",
          "widget-attributes": {
            "default": "org.janusgraph.graphdb.tinkerpop.JanusGraphIoRegistry"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Graph Source Name",
          "name": "graphSourceName",
          "widget-attributes": {
            "default": "g"
          }
        }
      ]
    }
  ],
  "outputs": []
}
