                    "properties": {
                        "referenceName": "JanusSink",
                        "hosts": "localhost",
                        "port": "8182",
                        "schema.row.field": "body_1",
                        "serializerClassName": "org.apache.tinkerpop.gremlin.driver.ser.GryoMessageSerializerV3d0",
                        "ioRegistries": "org.janusgraph.graphdb.tinkerpop.JanusGraphIoRegistry"
