{
    "name": "TestPipeline1",
    "description": "Data Pipeline Application",
    "artifact": {
        "name": "cdap-data-pipeline",
        "version": "6.6.0",
        "scope": "SYSTEM"
    },
    "config": {
        "resources": {
            "memoryMB": 2048,
            "virtualCores": 1
        },
        "driverResources": {
            "memoryMB": 2048,
            "virtualCores": 1
        },
        "connections": [
            {
                "from": "File",
                "to": "Wrangler"
            },
            {
                "from": "Wrangler",
                "to": "Janus"
            }
        ],
        "comments": [],
        "postActions": [],
        "properties": {},
        "processTimingEnabled": true,
        "stageLoggingEnabled": false,
        "stages": [
            {
                "name": "File",
                "plugin": {
                    "name": "File",
                    "type": "batchsource",
                    "label": "File",
                    "artifact": {
                        "name": "core-plugins",
                        "version": "2.8.0",
                        "scope": "SYSTEM"
                    },
                    "properties": {
                        "referenceName": "TestFile",
                        "path": "/home/sthota/Downloads/Sample-Spreadsheet-100-rows.csv",
                        "format": "text",
                        "sampleSize": "1000",
                        "skipHeader": "false",
                        "filenameOnly": "false",
                        "recursive": "false",
                        "ignoreNonExistingFolders": "false",
                        "fileEncoding": "UTF-8",
                        "schema": "{\"type\":\"record\",\"name\":\"fileRecord\",\"fields\":[{\"name\":\"offset\",\"type\":\"long\"},{\"name\":\"body\",\"type\":\"string\"}]}"
                    }
                },
                "outputSchema": "{\"type\":\"record\",\"name\":\"fileRecord\",\"fields\":[{\"name\":\"offset\",\"type\":\"long\"},{\"name\":\"body\",\"type\":\"string\"}]}",
                "id": "File",
                "type": "batchsource",
                "label": "File",
                "icon": "icon-file"
            },
            {
                "name": "Wrangler",
                "plugin": {
                    "name": "Wrangler",
                    "type": "transform",
                    "label": "Wrangler",
                    "artifact": {
                        "name": "wrangler-transform",
                        "version": "4.6.0",
                        "scope": "SYSTEM"
                    },
                    "properties": {
                        "field": "*",
                        "precondition": "false",
                        "directives": "parse-as-csv :body ',' false\ndrop :body",
                        "on-error": "fail-pipeline",
                        "schema": "{\"type\":\"record\",\"name\":\"record\",\"fields\":[{\"name\":\"body_1\",\"type\":[\"string\",\"null\"]},{\"name\":\"body_2\",\"type\":[\"string\",\"null\"]},{\"name\":\"body_3\",\"type\":[\"string\",\"null\"]},{\"name\":\"body_4\",\"type\":[\"string\",\"null\"]},{\"name\":\"body_5\",\"type\":[\"string\",\"null\"]},{\"name\":\"body_6\",\"type\":[\"string\",\"null\"]},{\"name\":\"body_7\",\"type\":[\"string\",\"null\"]},{\"name\":\"body_8\",\"type\":[\"string\",\"null\"]},{\"name\":\"body_9\",\"type\":[\"string\",\"null\"]},{\"name\":\"body_10\",\"type\":[\"string\",\"null\"]}]}",
                        "workspaceId": "2c725523-ee84-4831-8f57-4081764be38f"
                    }
                },
                "outputSchema": [
                    {
                        "name": "etlSchemaBody",
                        "schema": "{\"type\":\"record\",\"name\":\"record\",\"fields\":[{\"name\":\"body_1\",\"type\":[\"string\",\"null\"]},{\"name\":\"body_2\",\"type\":[\"string\",\"null\"]},{\"name\":\"body_3\",\"type\":[\"string\",\"null\"]},{\"name\":\"body_4\",\"type\":[\"string\",\"null\"]},{\"name\":\"body_5\",\"type\":[\"string\",\"null\"]},{\"name\":\"body_6\",\"type\":[\"string\",\"null\"]},{\"name\":\"body_7\",\"type\":[\"string\",\"null\"]},{\"name\":\"body_8\",\"type\":[\"string\",\"null\"]},{\"name\":\"body_9\",\"type\":[\"string\",\"null\"]},{\"name\":\"body_10\",\"type\":[\"string\",\"null\"]}]}"
                    }
                ],
                "inputSchema": [
                    {
                        "name": "File",
                        "schema": "{\"type\":\"record\",\"name\":\"fileRecord\",\"fields\":[{\"name\":\"offset\",\"type\":\"long\"},{\"name\":\"body\",\"type\":\"string\"}]}"
                    }
                ],
                "id": "Wrangler",
                "type": "transform",
                "label": "Wrangler",
                "icon": "icon-DataPreparation"
            },
            {
                "name": "Janus",
                "plugin": {
                    "name": "Janus",
                    "type": "batchsink",
                    "label": "Janus",
                    "artifact": {
                        "name": "janus-plugins",
                        "version": "0.1.0",
                        "scope": "USER"
